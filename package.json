{
  "name": "pyx",
  "version": "0.0.1",
  "description": "Python for Cloudflare Workers - Scientific computing at the edge with ~1s cold starts",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "python",
    "pyodide",
    "cloudflare",
    "workers",
    "edge",
    "scientific-computing",
    "numpy",
    "pandas",
    "wasm",
    "webassembly"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/pyx"
  },
  "homepage": "https://github.com/dot-do/pyx#readme",
  "bugs": {
    "url": "https://github.com/dot-do/pyx/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^25.0.6",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wrangler": "^3.99.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "hono": "^4.6.0"
  }
}
